{
  "footer": {
    "blocks": [
      "footer-layout.desktop",
      "footer-layout.mobile"
    ],
    "props": {
      "fullWidth": true
    }
  },
  "footer-layout.desktop": {
    "children": [
      "flex-layout.row#footer-1-desktop",
      "flex-layout.row#footer-2-desktop"
    ],
    "props": {
      "fullWidth": true
    }
  },
  "flex-layout.row#footer-1-desktop": {
    "children": [
      "vtex.menu@2.x:menu#Categorias-desktop",
      "vtex.menu@2.x:menu#Sabermas-desktop",
      "vtex.menu@2.x:menu#ayuda-desktop",
      "vtex.menu@2.x:menu#footer-bags",
      "footer-spacer",
      "social-networks"
    ],
    "props": {
      "blockClass": "footer",
      "paddingTop": 6,
      "paddingBottom": 3,
      "fullWidth": true
    }
  },
  "social-networks": {
    "props": {
      "socialNetworks": [
        {
          "name": "Facebook",
          "url": "https://www.facebook.com/vtexonline/"
        },
        {
          "name": "Instagram",
          "url": "https://www.instagram.com/vtextruecloud/"
        },
        {
          "name": "Twitter",
          "url": "https://twitter.com/vtexonline"
        },
        {
          "name": "Youtube",
          "url": "https://www.youtube.com/user/VTEXTV"
        }
      ]
    }
  },
  "flex-layout.row#footer-2-desktop": {
    "props": {
      "fullWidth": true,
      "rowGap": 1
      
    },
    "children": [
      "flex-layout.row#footer-2-1-desktop",
      "flex-layout.row#footer-2-2-desktop",
      "flex-layout.row#footer-2-3-desktop"
    ]
  },
  "flex-layout.row#footer-2-1-desktop": {
    "props": {
      "horizontalAlign": "center",      
      "fullWidth": true
    },
    "children": [
      "image#footerPagos"
    ]
  },
  "image#footerPagos": {
    "props": {
      "src": "https://diageocol.vtexassets.com/arquivos/metodos_pago.jpg",
      "blockClass": "image-footerPagos",
      "fullWidth": true,
      "width": 302,
      "height": 54
    }
  },
  "flex-layout.row#footer-2-2-desktop": {
    "props": {
      "horizontalAlign": "center",      
      "fullWidth": true
    },
    "children": [
      "rich-text#footerCopyright"
    ]
  },
  "rich-text#footerCopyright": {
    "props": {
      "text": "Â© Copyright 2020 TheBar.com.co. Todos los Derechos Reservados."
    }
  },
  "flex-layout.row#footer-2-3-desktop":{
    "props": {
      "horizontalAlign": "center",
      "fullWidth": true
    },
    "children": [
      "logo#footerBrandlive",
      "logo#footerVtex"   
    ]
  },
  "logo#footerBrandlive": {
    "props": {
      "url": "https://diageocol.vtexassets.com/arquivos/poweredBrandLive.jpg",
      "blockClass": "image-footerBrandlive",
      "fullWidth": true,
      "width": 196,
      "height": 91,
      "href": "httpS://www.brandlivecommerce.com/",
      "target": "_seft"
    }
  },
  "logo#footerVtex": {
    "props": {
      "url": "https://diageocol.vtexassets.com/arquivos/TechnologyVtex.jpg",
      "blockClass": "image-footerVtex",
      "fullWidth": true,
      "width": 177,
      "height": 100,
      "href": "https://vtex.com/co-es/"
    }
  },
  "footer-layout.mobile": {
    "children": [
      "flex-layout.row#1-footer-mobile",
      "flex-layout.row#2-footer-mobile"
    ]
  },
  "flex-layout.row#2-footer-mobile": {
    "children": [
      "flex-layout.col#footer-1-mobile"
    ],
    "props": {
      "blockClass": "payment-methods",
      "paddingTop": 4,
      "paddingBottom": 4
    }
  },
  "flex-layout.col#footer-1-mobile": {
    "children": [
      "accepted-payment-methods",
      "social-networks",
      "rich-text#footer-mobile"
    ],
    "props": {
      "blockClass": "payment-methods",
      "paddingTop": 4,
      "paddingBottom": 4,
      "rowGap": 4
    }
  },
  "flex-layout.row#1-footer-mobile": {
    "props": {
      "paddingTop": 4,
      "paddingBottom": 4
    },
    "children": [
      "vtex.menu@2.x:menu#footer-mobile"
    ]
  },
  "rich-text#footer-mobile": {
    "props": {
      "text": "All stock and product photos are from photos.icons8.com",
      "blockClass": "footer"
    }
  }
}