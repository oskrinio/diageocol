{
  "footer": {
    "blocks": [
      "footer-layout.desktop",
      "footer-layout.mobile"
    ],
    "props": {
      "fullWidth": true
    }
  },
  "footer-layout.desktop": {
    "children": [
      "flex-layout.row#footer-1-desktop",      
      "flex-layout.row#footer-2-1-desktop",
      "flex-layout.row#footer-2-2-desktop",
      "flex-layout.row#footer-2-3-desktop"
    ],
    "props": {
      "fullWidth": true
    }
  },
  "flex-layout.row#footer-1-desktop": {
    "children": [
      "vtex.menu@2.x:menu#Categorias-desktop",
      "vtex.menu@2.x:menu#Sabermas-desktop",
      "vtex.menu@2.x:menu#ayuda-desktop",
      "vtex.menu@2.x:menu#footer-bags",
      "footer-spacer",
      "social-networks"
    ],
    "props": {
      "blockClass": "footer",
      "paddingTop": 6,
      "paddingBottom": 3,
      "fullWidth": true
    }
  },
  "social-networks": {
    "props": {
      "socialNetworks": [
        {
          "name": "Facebook",
          "url": "https://www.facebook.com/vtexonline/"
        },
        {
          "name": "Instagram",
          "url": "https://www.instagram.com/vtextruecloud/"
        },
        {
          "name": "Twitter",
          "url": "https://twitter.com/vtexonline"
        },
        {
          "name": "Youtube",
          "url": "https://www.youtube.com/user/VTEXTV"
        }
      ]
    }
  },
  "flex-layout.row#footer-2-1-desktop": {
    "props": {
      "horizontalAlign": "center",      
      "fullWidth": true,
      "paddingTop": 6
    },
    "children": [
      "image#footerPagos"
    ]
  },
  "image#footerPagos": {
    "props": {
      "src": "https://diageocol.vtexassets.com/arquivos/metodos_pago.jpg",
      "blockClass": "image-footerPagos",
      "fullWidth": true,
      "width": 151,
      "height": 27
    }
  },
  "flex-layout.row#footer-2-2-desktop": {
    "props": {
      "horizontalAlign": "center",
      "fullWidth": true
    },
    "children": [
      "rich-text#footerCopyright"
    ]
  },
  "rich-text#footerCopyright": {
    "props": {
      "text": "Â© Copyright 2020 TheBar.com.co. Todos los Derechos Reservados.",
      "textAlignment": "CENTER",
      "textPosition": "CENTER"
    }
  },
  "flex-layout.row#footer-2-3-desktop": {
    "props": {
      "horizontalAlign": "center",
      "preventHorizontalStretch": true,
      "fullWidth": true,
      "paddingBottom": 5
    },
    "children": [
      "image#footerBrandlive",
      "image#footerVtex"
    ]
  },
  "image#footerBrandlive": {
    "props": {
      "src": "https://diageocol.vtexassets.com/arquivos/poweredBrandLive.jpg",
      "blockClass": "image-footerBrandlive",
      "fullWidth": true,
      "width": 118,
      "height": 55,
      "link": {
        "url": "https: //www.brandlivecommerce.com/",
        "openNewTab": true
      }
    }
  },
  "image#footerVtex": {
    "props": {
      "src": "https://diageocol.vtexassets.com/arquivos/TechnologyVtex.jpg",
      "blockClass": "image-footerVtex",
      "fullWidth": true,
      "width": 106,
      "height": 60,
      "link":{
        "url": "https://vtex.com/co-es/",
        "openNewTab": true
      }
    }
  },
  "footer-layout.mobile": {
    "children": [
      "flex-layout.row#footer-1-desktop",
      "flex-layout.row#footer-2-1-desktop",
      "flex-layout.row#footer-2-2-desktop",
      "flex-layout.row#footer-2-3-desktop"
    ],
    "props": {
      "fullWidth": true
    }
  }
}