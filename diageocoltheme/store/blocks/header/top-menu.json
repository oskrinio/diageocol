{
    "modal-trigger#bebidas": {
        "children": [
            "rich-text#bebidas",
            "modal-layout#bebidas"
        ],
        "props": {
            "trigger": "click"
        }
    },
    "rich-text#bebidas": {
        "props": {
            "text": "BEBIDAS",
            "blockClass": "mn-header"
        }
    },
    "modal-layout#bebidas": {
        "children": [
            "flex-layout.row#bebidas"
        ]
    },
    "flex-layout.row#bebidas": {
        "props": {
            "blockClass": "row-bebidas",
            "borderColor": "black",
            "colGap": 5,
            "rowGap": 3,
            "fullWidth": true
        },
        "children": [
            "flex-layout.col#bebidas-maltas",
            "flex-layout.col#bebidas-ginebra"
        ]
    },
    "flex-layout.col#bebidas-maltas":{
        "props":{
            "horizontalAlign": "center",
            "paddingRight": 20
        },
        "children":[
            "image#bebidas-maltas",
            "rich-text#bebidas-maltas"
        ]
    },
    "image#bebidas-maltas":{
        "props":{
            "src": "https://diageocol.vteximg.com.br/arquivos/bebidas-malta.png",
            "link": {
                "url": "/MALTAS",
                "openNewTab": false
            },
            "title": "MALTA",
            "maxHeight": "170"
        }
    },
    "rich-text#bebidas-maltas": {
        "props": {
            "text": "MALTAS",
            "blockClass": "text-bebidas",
            "textAlignment": "CENTER",
            "textPosition": "CENTER"
        }
